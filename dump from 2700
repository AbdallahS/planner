ite for details.\",\"academic_item_type\":\"Course\",\"no_enrolment\":\"false\",\"offering_detail\":{\"key\":\"pgrd\",\"fees\":{},\"offering_terms\":\"\",\"fees_table\":\"\",\"erg\":[],\"campus\":{\"name\":\"\"}},\"additional_information\":\"\",\"study_level_tooltip\":\"Postgraduate: A level of study that leads to the award of a Graduate Certificate, Graduate Diploma, Masters Degree or Doctorate.\",\"campus\":\"Sydney\",\"study_level_single\":{\"label\":\"Postgraduate\",\"value\":\"pgrd\"},\"code\":\"JURD7193\",\"attributes\":[],\"credit_points_header\":\"6 Units of Credit\",\"eqivalents\":[{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"JURD7193\",\"cl_id\":\"9e5df0bbdbf51c50eb6b147a3a961984\",\"key\":\"code\"},\"association_type\":{\"label\":\"Equivalents\",\"value\":\"9\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"LAWS8193\",\"cl_id\":\"834d70bbdbf51c50eb6b147a3a96192e\",\"key\":\"code\"},\"cl_id\":\"435d95c8db02d850358b403c3a96190a\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"State Crime, the Law and Civil Society\",\"assoc_cl_id\":\"834d70bbdbf51c50eb6b147a3a96192e\",\"assoc_short_title\":\"LAWS8193 - State Crime, the Law and Civil Society\",\"assoc_campus\":null,\"assoc_url\":\"/postgraduate/courses/2021/LAWS8193\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"LAWS8193\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Law and Justice\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]}],\"exclusion\":[{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"JURD7193\",\"cl_id\":\"9e5df0bbdbf51c50eb6b147a3a961984\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"CRIM3006\",\"cl_id\":\"a801462
061\",\"cl_id\":\"2ab5a9f3dbbd1c50eb6b147a3a9619b8\",\"key\":\"code\"},\"cl_id\":\"8dbd971fdbc39450358b403c3a961936\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Design in Application of Materials in Science and Engineering\",\"assoc_cl_id\":\"2ab5a9f3dbbd1c50eb6b147a3a9619b8\",\"assoc_short_title\":\"DPST1061 - Design in Application of Materials in Science and Engineering\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/DPST1061\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"DPST1061\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Global\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]}],\"exclusion\":[{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"MATS1192\",\"cl_id\":\"01bd971fdbc39450358b403c3a961932\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"DPST1061\",\"cl_id\":\"2ab5a9f3dbbd1c50eb6b147a3a9619b8\",\"key\":\"code\"},\"cl_id\":\"05bd971fdbc39450358b403c3a961937\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Design in Application of Materials in Science and Engineering\",\"assoc_cl_id\":\"2ab5a9f3dbbd1c50eb6b147a3a9619b8\",\"assoc_short_title\":\"DPST1061 - Design in Application of Materials in Science and Engineering\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/DPST1061\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"DPST1061\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Global\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl
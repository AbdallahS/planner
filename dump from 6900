al Physics (Honours)\",\"academic_item_type\":{\"label\":\"Course\",\"value\":\"subject\"},\"academic_item_pill_text\":\"\",\"academic_item_url\":\"/undergraduate/courses/2021/PHYS4142\",\"academic_item_state\":\"show\",\"academic_item_code\":\"PHYS4142\",\"order\":\"2\",\"links\":[]},{\"cl_id\":\"e929353fdb715c50eb6b147a3a9619b5\",\"exclude\":\"false\",\"vertical_grouping\":{\"label\":\"Core Course\",\"value\":\"CC\"},\"parent_connector\":{\"label\":\"AND\",\"value\":\"AND\"},\"parent_record\":{\"value\":\"Curriculum Structure Container: Research coursework\",\"cl_id\":\"a929353fdb715c50eb6b147a3a9619b0\",\"key\":\"title\",\"type\":\"x_f5sl_cl_cs_container\"},\"owning_structure\":{\"value\":\"2021.01.001\",\"cl_id\":\"2129353fdb715c50eb6b147a3a9619aa\",\"key\":\"version_code\"},\"child_record\":{\"value\":\"Course: PHYS4141\",\"cl_id\":\"29b14977db391c50eb6b147a3a961942\",\"key\":\"code\",\"type\":\"x_f5sl_cl_subjects\"},\"credit_points\":\"6\",\"child_table\":\"x_f5sl_cl_subjects\",\"description\":\"PHYS4141 - Quantum Mechanics (Honours)\",\"parent_table\":\"x_f5sl_cl_cs_container\",\"academic_item_active\":\"true\",\"abbr_name\":\"\",\"academic_item_version_name\":\"2021.01\",\"academic_item_credit_points\":\"6\",\"abbreviated_name_and_major\":null,\"academic_item_name\":\"Quantum Mechanics (Honours)\",\"academic_item_type\":{\"label\":\"Course\",\"value\":\"subject\"},\"academic_item_pill_text\":\"\",\"academic_item_url\":\"/undergraduate/courses/2021/PHYS4141\",\"academic_item_state\":\"show\",\"academic_item_code\":\"PHYS4141\",\"order\":\"1\",\"links\":[]},{\"cl_id\":\"f929353fdb715c50eb6b147a3a9619be\",\"exclude\":\"false\",\"vertical_grouping\":{\"label\":\"Core Course\",\"value\":\"CC\"},\"parent_connector\":{\"label\":\"AND\",\"value\":\"AND\"},\"parent_record\":{\"value\":\"Curriculum Structure Container: Research coursework\",\"cl_id\":\"a929353fdb715c50eb6b147a3a9619b0\",\"key\":\"title\",\"type\":\"x_f5sl_cl_cs_container\"},\"owning_structure\":{\"val
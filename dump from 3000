\",\"published_in_handbook\":{\"label\":\"Yes\",\"value\":\"1\"},\"name\":\"Faculty of Law and Justice\",\"ext_id\":\"\",\"active\":\"true\",\"parent\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"code\":\"LAW\",\"description\":\"A leader in progressive and rigorous legal education. Inspired by principles of justice, we study law in action and make a difference in this world.\",\"url\":\"https://www.law.unsw.edu.au\",\"name_url\":\"Faculty of Law and Justice\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"5550\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"LAWS3115-8\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 3\",\"cl_id\":\"601633b7db7993009c2c403c3a9619ab\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Undergraduate\",\"value\":\"ugrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"LAWS3115\",\"cl_id\":\"ea3db601dbe71810eb6b147a3a9619e6\",\"key\":\"code\"},\"fees_commonwealth\":\"1813\",\"fees_international\":\"5550\",\"course_restrictions\":\"\",\"cl_id\":\"ea3db601dbe71810eb6b147a3a9619e8\"}],\"academic_calendar_tooltip\":\"Consists of three standard terms, each with ten weeks teaching, and an optional five week summer term.\u00a0 Some programs in Medicine, AGSM, and UNSW Global have different term dates.\u00a0 See the university website for details.\",\"academic_item_type\":\"Course\",\"no_enrolment\":\"false\",\"offering_detail\":{\"key\":\"ugrd\",\"fees\":{\"fees_commonwealth_link\":{\"label\":\"Commonwealth Supported Students (if applicable)\",\"value\":\"https://student.unsw.edu.au/fees-student-contribution-rates\"},\"fees_commonwealth\":\"1813\",\"fees_domestic_link\":{\"label\":\"Domestic Students\",\"value\":\"https://student.unsw.edu.au/fees-domestic-full-fee-paying\"},\"fees_domestic\":\"5550\"
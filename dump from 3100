es\",\"value\":\"1\"},\"name\":\"School of Global and Public Law\",\"ext_id\":\"\",\"active\":\"true\",\"parent\":{\"value\":\"Faculty of Law and Justice\",\"cl_id\":\"09d355af1bbae810d044ffbbdc4bcbe0\",\"key\":\"name\"},\"code\":\"GPLAW\",\"description\":\"\",\"url\":\"\",\"name_url\":\"\",\"name_no_url\":\"School of Global and Public Law\"}],\"hb_delivery_variations\":[{\"delivery_collaborators\":{\"label\":\"Intensive\",\"value\":\"IN\"},\"delivery_mode\":{\"value\":\"Blended in-person / online\",\"cl_id\":\"1238f5ffdbc90010ad972ed344961986\",\"key\":\"description\"},\"handbook_notes\":\"\",\"delivery_name\":\"BLENDED-IN\",\"contact_hours\":\"Refer to UNSW Timetable\",\"delivery_variation_label\":\"\",\"cl_id\":\"aa76b8f7dbb51c50eb6b147a3a961929\",\"display_name\":\"Blended in-person / online - Intensive\"},{\"delivery_collaborators\":{\"label\":\"Standard (usually weekly or fortnightly)\",\"value\":\"ST\"},\"delivery_mode\":{\"value\":\"Blended in-person / online\",\"cl_id\":\"1238f5ffdbc90010ad972ed344961986\",\"key\":\"description\"},\"handbook_notes\":\"\",\"delivery_name\":\"BLENDED-ST\",\"contact_hours\":\"Refer to UNSW Timetable\",\"delivery_variation_label\":\"\",\"cl_id\":\"ae76b8f7dbb51c50eb6b147a3a961926\",\"display_name\":\"Blended in-person / online - Standard (usually weekly or fortnightly)\"}],\"enrolment_rules\":[{\"active\":\"true\",\"academic_item\":{\"value\":\"LAWS3396\",\"cl_id\":\"7d7678f7dbb51c50eb6b147a3a9619b7\",\"key\":\"code\"},\"type\":{\"label\":\"Enrolment Requirements\",\"value\":\"info\"},\"order\":\"\",\"cl_id\":\"06c0e140db42d850358b403c3a961908\",\"description\":\"Prerequisite: Family Law (LAWS3391) and completion of 78 UOC in LAWS courses.<br/><br/>\"}],\"faculty_detail\":[{\"cl_id\":\"09d355af1bbae810d044ffbbdc4bcbe0\",\"published_in_handbook\":{\"label\":\"Yes\",\"value\":\"1\"},\"name\":\"Faculty of Law and Justice\",\"ext_id\":\"\",\"active\":\"true\",\"parent\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"code\":\"LAW\",\"description\":\"A leader in progressive and rigorous legal education. Inspired by principles of justice, we study law in action and make a difference in this world.\",\"url\":\"https://www.law.unsw.edu.au\",\"name_url\":\"Faculty of Law and Justice\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"5550\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"LAWS3396-4\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Summer Term\",\"cl_id\":\"b8f53777db7993009c2c403c3a9619e4\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Undergraduate\",\"value\":\"ugrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"LAWS3396\",\"cl_id\":\"7d7678f7dbb51c50eb6b147a3a9619b7\",\"key\":\"code\"},\"fees_commonwealth\":\"1813\",\"fees_international\":\"5550\",\"course_restrictions\":\"\",\"cl_id\":\"0a7fa144db82d850358b403c3a961953\"},{\"fees_domestic\":\"5550\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"LAWS3396-7\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 2\",\"cl_id\":\"03067bf7db7993009c2c403c3a961997\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Undergraduate\",\"value\":\"ugrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"LAWS3396\",\"cl_id\":\"7d7678f7dbb51c50eb6b147a3a9619b7\",\"key\":\"code\"},\"fees_commonwealth\":\"1813\",\"fees_international\":\"5550\",\"course_restrictions\":\"\",\"cl_id\":\"6039ed0cdb42d850358b403c3a961937\"},{\"fees_domestic\":\"5550\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"LAWS3396-8\",\"clarification_to_appear_in_handbook\":\"\",\"display_na
-term Examination\",\"assessment_type\":{\"label\":\"Examination\",\"value\":\"examination\"},\"assessment_id\":\"2\",\"weighting\":\"25\",\"description\":null,\"learning_outcomes\":[{\"value\":\"CLO2\",\"cl_id\":\"30a0193fdbb91c50eb6b147a3a9619e5\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"7ca0193fdbb91c50eb6b147a3a9619e7\",\"key\":\"code\"},{\"value\":\"CLO1\",\"cl_id\":\"f0a0193fdbb91c50eb6b147a3a9619e2\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"24a0193fdbb91c50eb6b147a3a9619d4\",\"links\":[]},{\"individual\":{\"label\":null,\"value\":null},\"assessment_name\":\"Assignments and Quizzes\",\"assessment_type\":{\"label\":\"Other\",\"value\":\"other\"},\"assessment_id\":\"1\",\"weighting\":\"30\",\"description\":null,\"learning_outcomes\":[{\"value\":\"CLO2\",\"cl_id\":\"30a0193fdbb91c50eb6b147a3a9619e5\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"7ca0193fdbb91c50eb6b147a3a9619e7\",\"key\":\"code\"},{\"value\":\"CLO1\",\"cl_id\":\"f0a0193fdbb91c50eb6b147a3a9619e2\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"2ca0193fdbb91c50eb6b147a3a9619cf\",\"links\":[]},{\"individual\":{\"label\":null,\"value\":null},\"assessment_name\":\"Final Examination\",\"assessment_type\":{\"label\":\"Examination\",\"value\":\"examination\"},\"assessment_id\":\"3\",\"weighting\":\"45\",\"description\":null,\"learning_outcomes\":[{\"value\":\"CLO2\",\"cl_id\":\"30a0193fdbb91c50eb6b147a3a9619e5\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"7ca0193fdbb91c50eb6b147a3a9619e7\",\"key\":\"code\"},{\"value\":\"CLO1\",\"cl_id\":\"f0a0193fdbb91c50eb6b147a3a9619e2\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"2ca0193fdbb91c50eb6b147a3a9619d8\",\"links\":[]}]}],\"inactive_course_alert\":null,\"timetableURL\":\"http://timetable.unsw.edu.au/2021/PTRL1010.html\",\"subject_sample_text_staticcontent\":
31\",\"cl_id\":\"f7f325ffdb7d1c50eb6b147a3a961933\",\"key\":\"code\",\"type\":\"x_f5sl_cl_subjects\"},\"credit_points\":\"6\",\"child_table\":\"x_f5sl_cl_subjects\",\"description\":\"MATH1131 - Mathematics 1A\",\"parent_table\":\"x_f5sl_cl_cs_container\",\"academic_item_active\":\"true\",\"abbr_name\":\"\",\"academic_item_version_name\":\"2021.01\",\"academic_item_credit_points\":\"6\",\"abbreviated_name_and_major\":null,\"academic_item_name\":\"Mathematics 1A\",\"academic_item_type\":{\"label\":\"Course\",\"value\":\"subject\"},\"academic_item_pill_text\":\"\",\"academic_item_url\":\"/undergraduate/courses/2021/MATH1131\",\"academic_item_state\":\"show\",\"academic_item_code\":\"MATH1131\",\"order\":\"2\",\"links\":[]},{\"cl_id\":\"8aa5f1b1db872090be4672f5f396199f\",\"exclude\":\"false\",\"vertical_grouping\":{\"label\":\"One of the following\",\"value\":\"one_of_the_following\"},\"parent_connector\":{\"label\":\"OR\",\"value\":\"OR\"},\"parent_record\":{\"value\":\"Curriculum Structure Container: One of the following:\",\"cl_id\":\"c6a5f1b1db872090be4672f5f396199f\",\"key\":\"title\",\"type\":\"x_f5sl_cl_cs_container\"},\"owning_structure\":{\"value\":\"2021.01.006\",\"cl_id\":\"06a5f1b1db872090be4672f5f396199e\",\"key\":\"version_code\"},\"child_record\":{\"value\":\"Course: MATH1041\",\"cl_id\":\"87e4793ddb87a410fe4b322af49619aa\",\"key\":\"code\",\"type\":\"x_f5sl_cl_subjects\"},\"credit_points\":\"6\",\"child_table\":\"x_f5sl_cl_subjects\",\"description\":\"MATH1041 - Statistics for Life and Social Sciences\",\"parent_table\":\"x_f5sl_cl_cs_container\",\"academic_item_active\":\"true\",\"abbr_name\":\"\",\"academic_item_version_name\":\"2021.02\",\"academic_item_credit_points\":\"6\",\"abbreviated_name_and_major\":null,\"academic_item_name\":\"Statistics for Life and Social Sciences\",\"academic_item_type\":{\"label\":\"Course\",\"value\":\"subject\"},\"academic_item_pill_text\":\"\",\"academic_item_url\":\"/undergraduate/courses/2021/MATH1041\",\"academic_ite
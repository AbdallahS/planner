y\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"cbad16e3dbb99850eb6b147a3a96193f\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"faad16e3dbb99850eb6b147a3a961916\",\"links\":[]},{\"individual\":{\"label\":null,\"value\":null},\"assessment_name\":\"Final exam\",\"assessment_type\":{\"label\":\"Assignment\",\"value\":\"assignment\"},\"assessment_id\":\"4\",\"weighting\":\"30\",\"description\":null,\"learning_outcomes\":[{\"value\":\"CLO4\",\"cl_id\":\"0bad16e3dbb99850eb6b147a3a961942\",\"key\":\"code\"},{\"value\":\"CLO1\",\"cl_id\":\"43ad16e3dbb99850eb6b147a3a96193a\",\"key\":\"code\"},{\"value\":\"CLO5\",\"cl_id\":\"57ad16e3dbb99850eb6b147a3a961945\",\"key\":\"code\"},{\"value\":\"CLO2\",\"cl_id\":\"8fad16e3dbb99850eb6b147a3a96193c\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"cbad16e3dbb99850eb6b147a3a96193f\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"faad16e3dbb99850eb6b147a3a96191f\",\"links\":[]}]}],\"inactive_course_alert\":null,\"timetableURL\":\"http://timetable.unsw.edu.au/2021/FINS5593.html\",\"subject_sample_text_staticcontent\":\"To access course outline, please visit:\",\"links\":[]}", "keywords": "finance,capital markets,trading and trading venues,liquidity,information and price discovery", "studyLevel": "Postgraduate", "deliveryMethodAlt": "1238f5ffdbc90010ad972ed344961986", "inode": "bda29602-2be8-445a-9cf1-c7e2c21c2f4a", "ascedDetailed": "064569564ff5d7009e2bfd501310c71d", "host": "f59fc109-4aaa-40e0-bdcc-7039d31533f8", "ascedBroadSingle": "dd350293db96df002e4c126b3a961909", "locked": false, "stInode": "8edd6955-283b-4e77-a23f-b968d2642ec1", "contentType": "unsw_psubject", "identifier": "b4cd6684-dbaf-41f4-bd7c-91b33c1c1f54", "level": "subject", "active": "1", "version": "1", "folder": "SYSTEM_FOLDER", "hasTitleImage": false, "sortOrder": 0, "generalEducation": "false", "status": "Active", "hostName": "www.handbook.unsw.edu.
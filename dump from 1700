usions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"TABL2710\",\"cl_id\":\"1b6a6dffdbbd1c50eb6b147a3a96196c\",\"key\":\"code\"},\"cl_id\":\"b4c49e31db70a81055a93324f39619e8\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Business and the Law\",\"assoc_cl_id\":\"b4c49e31db70a81055a93324f39619e8\",\"assoc_short_title\":\"TABL2710 - Business and the Law\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/TABL2710\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"TABL2710\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Business School\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"DPBS1710\",\"cl_id\":\"6a62696adb84a490eb6b147a3a961983\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"TABL1710\",\"cl_id\":\"4778e369db025490eb6b147a3a9619eb\",\"key\":\"code\"},\"cl_id\":\"c0a41e31db70a81055a93324f396198e\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Business and the Law\",\"assoc_cl_id\":\"c0a41e31db70a81055a93324f396198e\",\"assoc_short_title\":\"TABL1710 - Business and the Law\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/TABL1710\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"TABL1710\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Business School\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]}],\"search_title\":\"DPBS1710 - Business and the Law\",\"description\":\"<p>Business and the Law is\u00a0a foundation course in
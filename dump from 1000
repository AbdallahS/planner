ue\",\"assoc_title\":\"Higher Chemistry 1B: Elements, Compounds and Life\",\"assoc_cl_id\":\"6722cb6fdb4a9410358b403c3a9619a1\",\"assoc_short_title\":\"CHEM1041 - Higher Chemistry 1B: Elements, Compounds and Life\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/CHEM1041\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"CHEM1041\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Science\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"CHEM1021\",\"cl_id\":\"ab1f39abdb359850eb6b147a3a96194d\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"CHEM1821\",\"cl_id\":\"751c30f7dbf51c50eb6b147a3a9619eb\",\"key\":\"code\"},\"cl_id\":\"b429dd44db02d850358b403c3a9619b8\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Engineering Chemistry 1B\",\"assoc_cl_id\":\"751c30f7dbf51c50eb6b147a3a9619eb\",\"assoc_short_title\":\"CHEM1821 - Engineering Chemistry 1B\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/CHEM1821\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"CHEM1821\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Science\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"CHEM1021\",\"cl_id\":\"ab1f39abdb359850eb6b147a3a96194d\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"CHEM1061\",\"cl_id\":\"4522d363db35d850eb6b147
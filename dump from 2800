ue\",\"parent\":{\"value\":\"Faculty of Law and Justice\",\"cl_id\":\"09d355af1bbae810d044ffbbdc4bcbe0\",\"key\":\"name\"},\"code\":\"GPLAW\",\"description\":\"\",\"url\":\"\",\"name_url\":\"\",\"name_no_url\":\"School of Global and Public Law\"}],\"hb_delivery_variations\":[{\"delivery_collaborators\":{\"label\":\"Standard (usually weekly or fortnightly)\",\"value\":\"ST\"},\"delivery_mode\":{\"value\":\"Blended in-person / online\",\"cl_id\":\"1238f5ffdbc90010ad972ed344961986\",\"key\":\"description\"},\"handbook_notes\":\"\",\"delivery_name\":\"BLENDED-ST\",\"contact_hours\":\"Refer to UNSW Timetable\",\"delivery_variation_label\":\"\",\"cl_id\":\"e11e0af5dbeb9810eb6b147a3a961980\",\"display_name\":\"Blended in-person / online - Standard (usually weekly or fortnightly)\"}],\"enrolment_rules\":[{\"active\":\"true\",\"academic_item\":{\"value\":\"JURD7450\",\"cl_id\":\"691e0af5dbeb9810eb6b147a3a96197e\",\"key\":\"code\"},\"type\":{\"label\":\"Enrolment Requirements\",\"value\":\"info\"},\"order\":\"\",\"cl_id\":\"6d1e4af5dbeb9810eb6b147a3a96194b\",\"description\":\"Prerequisites: completion of 36 UOC of JURD courses including Federal Constitutional Law (JURD7250) for students enrolled prior to 2013. For students enrolled after 2013, completion of 72 UOC of JURD courses including Fed.Con. (JURD7250).<br/><br/>\"}],\"faculty_detail\":[{\"cl_id\":\"09d355af1bbae810d044ffbbdc4bcbe0\",\"published_in_handbook\":{\"label\":\"Yes\",\"value\":\"1\"},\"name\":\"Faculty of Law and Justice\",\"ext_id\":\"\",\"active\":\"true\",\"parent\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"code\":\"LAW\",\"description\":\"A leader in progressive and rigorous legal education. Inspired by principles of justice, we study law in action and make a difference in this world.\",\"url\":\"https://www.law.unsw.edu.au\",\"name_url\":\"Faculty of Law and Justice\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"5250\",\"publish\":\"true\",\"location\":{\"val
at Assessment\",\"assessment_type\":{\"label\":\"Assignment\",\"value\":\"assignment\"},\"assessment_id\":\"4\",\"weighting\":\"15\",\"description\":\"<p>Marked online assignment</p>\\n\",\"learning_outcomes\":[{\"value\":\"CLO1\",\"cl_id\":\"08f7d9ffdbf91c50eb6b147a3a9619a0\",\"key\":\"code\"},{\"value\":\"CLO5\",\"cl_id\":\"1cf7d9ffdbf91c50eb6b147a3a9619ab\",\"key\":\"code\"},{\"value\":\"CLO2\",\"cl_id\":\"44f7d9ffdbf91c50eb6b147a3a9619a3\",\"key\":\"code\"},{\"value\":\"CLO6\",\"cl_id\":\"58f7d9ffdbf91c50eb6b147a3a9619ae\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"80f7d9ffdbf91c50eb6b147a3a9619a6\",\"key\":\"code\"},{\"value\":\"CLO7\",\"cl_id\":\"94f7d9ffdbf91c50eb6b147a3a9619b1\",\"key\":\"code\"},{\"value\":\"CLO4\",\"cl_id\":\"dcf7d9ffdbf91c50eb6b147a3a9619a8\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"73e7d9ffdbf91c50eb6b147a3a961997\",\"links\":[]},{\"individual\":{\"label\":null,\"value\":null},\"assessment_name\":\"Emergency Scenario\",\"assessment_type\":{\"label\":\"Assignment\",\"value\":\"assignment\"},\"assessment_id\":\"1\",\"weighting\":\"15\",\"description\":\"<p>Marked online assignment</p>\\n\",\"learning_outcomes\":[{\"value\":\"CLO1\",\"cl_id\":\"08f7d9ffdbf91c50eb6b147a3a9619a0\",\"key\":\"code\"},{\"value\":\"CLO5\",\"cl_id\":\"1cf7d9ffdbf91c50eb6b147a3a9619ab\",\"key\":\"code\"},{\"value\":\"CLO2\",\"cl_id\":\"44f7d9ffdbf91c50eb6b147a3a9619a3\",\"key\":\"code\"},{\"value\":\"CLO6\",\"cl_id\":\"58f7d9ffdbf91c50eb6b147a3a9619ae\",\"key\":\"code\"},{\"value\":\"CLO3\",\"cl_id\":\"80f7d9ffdbf91c50eb6b147a3a9619a6\",\"key\":\"code\"},{\"value\":\"CLO7\",\"cl_id\":\"94f7d9ffdbf91c50eb6b147a3a9619b1\",\"key\":\"code\"},{\"value\":\"CLO4\",\"cl_id\":\"dcf7d9ffdbf91c50eb6b147a3a9619a8\",\"key\":\"code\"}],\"delivery_variations\":[],\"applies_to_all_delivery_variations\":\"true\",\"cl_id\":\"f7e7d9ffdbf91c50eb6b147a3a961989\",\"links\":[]},{\"individual\":{\"label\":null,\"value
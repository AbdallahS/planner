\"},\"delivery_mode\":{\"value\":\"Blended in-person / online\",\"cl_id\":\"1238f5ffdbc90010ad972ed344961986\",\"key\":\"description\"},\"handbook_notes\":\"\",\"delivery_name\":\"BLENDED-IN\",\"contact_hours\":\"Refer to UNSW Timetable\",\"delivery_variation_label\":\"\",\"cl_id\":\"67657c37dbb51c50eb6b147a3a96192b\",\"display_name\":\"Blended in-person / online - Intensive\"},{\"delivery_collaborators\":{\"label\":\"Standard (usually weekly or fortnightly)\",\"value\":\"ST\"},\"delivery_mode\":{\"value\":\"Blended in-person / online\",\"cl_id\":\"1238f5ffdbc90010ad972ed344961986\",\"key\":\"description\"},\"handbook_notes\":\"\",\"delivery_name\":\"BLENDED-ST\",\"contact_hours\":\"Refer to UNSW Timetable\",\"delivery_variation_label\":\"\",\"cl_id\":\"a3657c37dbb51c50eb6b147a3a96191c\",\"display_name\":\"Blended in-person / online - Standard (usually weekly or fortnightly)\"}],\"enrolment_rules\":[{\"active\":\"true\",\"academic_item\":{\"value\":\"LAWS8091\",\"cl_id\":\"72653c37dbb51c50eb6b147a3a9619ba\",\"key\":\"code\"},\"type\":{\"label\":\"Enrolment Requirements\",\"value\":\"info\"},\"order\":\"\",\"cl_id\":\"5cef19ccdb02d850358b403c3a9619e5\",\"description\":\"Prerequisite: Academic Program must be either 9200 or 9201.<br/><br/>\"}],\"faculty_detail\":[{\"cl_id\":\"09d355af1bbae810d044ffbbdc4bcbe0\",\"published_in_handbook\":{\"label\":\"Yes\",\"value\":\"1\"},\"name\":\"Faculty of Law and Justice\",\"ext_id\":\"\",\"active\":\"true\",\"parent\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"code\":\"LAW\",\"description\":\"A leader in progressive and rigorous legal education. Inspired by principles of justice, we study law in action and make a difference in this world.\",\"url\":\"https://www.law.unsw.edu.au\",\"name_url\":\"Faculty of Law and Justice\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"LAWS8091-8\",\"clarifica
021/MNGT5251\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"MNGT5251\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Business School\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"MBAX9114\",\"cl_id\":\"825109e5db075050358b403c3a961954\",\"key\":\"code\"},\"association_type\":{\"label\":\"Equivalents\",\"value\":\"9\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"GBAT9114\",\"cl_id\":\"3fd72aefdbf99850eb6b147a3a961947\",\"key\":\"code\"},\"cl_id\":\"c65109e5db075050358b403c3a96195c\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"false\",\"assoc_title\":\"Marketing Management\",\"assoc_cl_id\":\"3fd72aefdbf99850eb6b147a3a961947\",\"assoc_short_title\":\"GBAT9114 - Marketing Management\",\"assoc_campus\":null,\"assoc_url\":\"/postgraduate/courses/2021/GBAT9114\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"inactive\",\"assoc_award_single\":\"\",\"assoc_code\":\"GBAT9114\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"UNSW Business School\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"MBAX9114\",\"cl_id\":\"825109e5db075050358b403c3a961954\",\"key\":\"code\"},\"association_type\":{\"label\":\"Equivalents\",\"value\":\"9\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"MNGT6251\",\"cl_id\":\"7433e17fdb7d1c50eb6b147a3a9619c2\",\"key\":\"code\"},\"cl_id\":\"d016c0f2dbbfd850eb6b147a3a96194a\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Marketing Management\",\"assoc_cl_id\":\"d016c0f2dbbfd850eb6b147a3a96194a\",\"assoc_short_title\":\"MNGT6251 - Marketing Manag
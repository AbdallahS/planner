its of Credit\",\"eqivalents\":[],\"exclusion\":[{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"JURD7434\",\"cl_id\":\"71253037dbb51c50eb6b147a3a961977\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"LAWS3434\",\"cl_id\":\"a7257037dbb51c50eb6b147a3a9619f9\",\"key\":\"code\"},\"cl_id\":\"3c4bd908db02d850358b403c3a96197c\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Research Thesis: Extended\",\"assoc_cl_id\":\"a7257037dbb51c50eb6b147a3a9619f9\",\"assoc_short_title\":\"LAWS3434 - Research Thesis: Extended\",\"assoc_campus\":null,\"assoc_url\":\"/undergraduate/courses/2021/LAWS3434\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"LAWS3434\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Law and Justice\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"JURD7434\",\"cl_id\":\"71253037dbb51c50eb6b147a3a961977\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"LAWS8422\",\"cl_id\":\"377b3877dbf51c50eb6b147a3a961913\",\"key\":\"code\"},\"cl_id\":\"c14bd908db02d850358b403c3a96197f\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"Research Thesis: Extended\",\"assoc_cl_id\":\"377b3877dbf51c50eb6b147a3a961913\",\"assoc_short_title\":\"LAWS8422 - Research Thesis: Extended\",\"assoc_campus\":null,\"assoc_url\":\"/postgraduate/courses/2021/LAWS8422\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"LAWS8422\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"
urs.\",\"url\":\"http://www.business.unsw.edu.au\",\"name_url\":\"UNSW Business School\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"4410\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"MFIN6210-6\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 1\",\"cl_id\":\"bc067bf7db7993009c2c403c3a961994\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Postgraduate\",\"value\":\"pgrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"MFIN6210\",\"cl_id\":\"aa52a223dbf99850eb6b147a3a961987\",\"key\":\"code\"},\"fees_commonwealth\":\"1813\",\"fees_international\":\"6030\",\"course_restrictions\":\"\",\"cl_id\":\"d00a7dc0dbc2d850358b403c3a961958\"},{\"fees_domestic\":\"4410\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"MFIN6210-8\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 3\",\"cl_id\":\"601633b7db7993009c2c403c3a9619ab\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Postgraduate\",\"value\":\"pgrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"MFIN6210\",\"cl_id\":\"aa52a223dbf99850eb6b147a3a961987\",\"key\":\"code\"},\"fees_commonwealth\":\"1813\",\"fees_international\":\"6030\",\"course_restrictions\":\"\",\"cl_id\":\"eb5f3d88dbc2d850358b403c3a9619bc\"},{\"fees_domestic\":\"4410\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"MFIN6210-7\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 2\",\"cl_id\":\"03067bf7db7993009c2c403c3a961997\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\"
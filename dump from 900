ent\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"code\":\"SCI\",\"description\":\"Discover and describe the world around you through experimentation and observation. Balance theory and practical experience to foster career goals.\",\"url\":\"http://www.science.unsw.edu.au\",\"name_url\":\"Faculty of Science\",\"name_no_url\":\"\"}],\"requisites\":[],\"unit_offering\":[{\"fees_domestic\":\"\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"BIOS9123-7\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Term 2\",\"cl_id\":\"03067bf7db7993009c2c403c3a961997\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Postgraduate\",\"value\":\"pgrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"BIOS9123\",\"cl_id\":\"5952783fdb751c50eb6b147a3a961929\",\"key\":\"code\"},\"fees_commonwealth\":\"\",\"fees_international\":\"\",\"course_restrictions\":\"\",\"cl_id\":\"28e7e1c8db42d850358b403c3a961998\"},{\"fees_domestic\":\"\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"BIOS9123-4\",\"clarification_to_appear_in_handbook\":\"\",\"display_name\":\"\",\"teaching_period\":{\"value\":\"Summer Term\",\"cl_id\":\"b8f53777db7993009c2c403c3a9619e4\",\"key\":\"name\"},\"attendance_mode\":{\"value\":\"\",\"cl_id\":null,\"key\":\"description\"},\"quota_number\":\"\",\"study_level\":{\"label\":\"Postgraduate\",\"value\":\"pgrd\"},\"self_enrol\":\"true\",\"academic_item\":{\"value\":\"BIOS9123\",\"cl_id\":\"5952783fdb751c50eb6b147a3a961929\",\"key\":\"code\"},\"fees_commonwealth\":\"\",\"fees_international\":\"\",\"course_restrictions\":\"\",\"cl_id\":\"6f703984db82d850358b403c3a961973\"},{\"fees_domestic\":\"\",\"publish\":\"true\",\"location\":{\"value\":\"\",\"cl_id\":null,\"key\":\"name\"},\"name\":\"BIOS9123-6\",\"clarification_to_appear
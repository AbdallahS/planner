e\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"JURD7691\",\"cl_id\":\"9e1338bfdb751c50eb6b147a3a961999\",\"key\":\"code\"},\"cl_id\":\"551bd508db02d850358b403c3a96198f\",\"assoc_credit_points\":\"6\",\"assoc_active\":\"true\",\"assoc_title\":\"International Law of Equality and Discrimination\",\"assoc_cl_id\":\"9e1338bfdb751c50eb6b147a3a961999\",\"assoc_short_title\":\"JURD7691 - International Law of Equality and Discrimination\",\"assoc_campus\":null,\"assoc_url\":\"/postgraduate/courses/2021/JURD7691\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"show\",\"assoc_award_single\":\"\",\"assoc_code\":\"JURD7691\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Law and Justice\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":[]},{\"dependent_type\":{\"label\":null,\"value\":null},\"ai_cl_id\":{\"value\":\"JURD7412\",\"cl_id\":\"c462f83fdb751c50eb6b147a3a961900\",\"key\":\"code\"},\"association_type\":{\"label\":\"Exclusions\",\"value\":\"10\"},\"association_desc\":\"\",\"associated_ai_cl_id\":{\"value\":\"LAWS8392\",\"cl_id\":\"20b60e63dbb59850eb6b147a3a9619fe\",\"key\":\"code\"},\"cl_id\":\"5d1bd508db02d850358b403c3a96198c\",\"assoc_credit_points\":\"4\",\"assoc_active\":\"false\",\"assoc_title\":\"Issues in Discrimination Law\",\"assoc_cl_id\":\"20b60e63dbb59850eb6b147a3a9619fe\",\"assoc_short_title\":\"LAWS8392 - Issues in Discrimination Law\",\"assoc_campus\":null,\"assoc_url\":\"/postgraduate/courses/2021/LAWS8392\",\"assoc_class\":\"Course\",\"assoc_award_titles\":null,\"assoc_state\":\"inactive\",\"assoc_award_single\":\"\",\"assoc_code\":\"LAWS8392\",\"assoc_class_translated_subjects\":\"Course\",\"assoc_faculty\":\"Faculty of Law and Justice\",\"assoc_class_translated\":\"Program\",\"assoc_duration_hb_display\":\"\",\"assoc_current_version\":{},\"links\":